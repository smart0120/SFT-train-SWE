# Copy this file to .env and fill in your values.
# Do not commit .env to version control.

# --- Hugging Face (required for gated models like Llama) ---
# Get token: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here

# --- Weights & Biases (optional; set to "disabled" to turn off) ---
# Get API key: https://wandb.ai/authorize
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=sft-lora
WANDB_ENTITY=your_wandb_username_or_team

# --- Optional overrides (override config/train_config.yaml) ---
# MODEL_ID=meta-llama/Llama-3.2-1B-Instruct
# ADAPTER_OUTPUT_DIR=outputs/adapters
# MERGED_OUTPUT_DIR=outputs/merged
# RUN_NAME=llama-3.2-1b-lora
